{
    "name": "watson/breadcrumbs",
    "description": "Breadcrumbs made easy for Laravel.",
    "keywords": ["laravel", "breadcrumbs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwight@studentservices.com.au"
        }
    ],
    "require": {
        "php": "^7.2.5||^8.0",
        "illuminate/routing": "^6.0||^7.0||^8.0",
        "illuminate/support": "^6.0||^7.0||^8.0",
        "illuminate/view": "^6.0||^7.0||^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1||^1.4.2",
        "phpunit/phpunit": "^8.2.3||^9.0",
        "orchestra/testbench": "^4.0||^5.0||^6.1"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Watson\\Breadcrumbs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Breadcrumbs\\ServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Watson\\Breadcrumbs\\Facade"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
