{
    "name": "watson/breadcrumbs",
    "description": "Breadcrumbs made easy for Laravel.",
    "keywords": [
        "laravel",
        "breadcrumbs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwight@studentservices.com.au"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "illuminate/routing": "^6.9||^7.0||^8.0",
        "illuminate/support": "^6.9||^7.0||^8.0",
        "illuminate/view": "^6.9||^7.0||^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^4.5||^5.0||^6.1"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Watson\\Breadcrumbs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Breadcrumbs\\ServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Watson\\Breadcrumbs\\Facades\\Breadcrumbs",
                "Trail": "Watson\\Breadcrumbs\\Facades\\Trail"
            }
        }
    },
    "minimum-stability": "stable"
}
