{
    "name": "watson/breadcrumbs",
    "description": "Breadcrumbs made easy for Laravel.",
    "keywords": ["laravel", "breadcrumbs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwight@studentservices.com.au"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Watson\\Breadcrumbs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Breadcrumbs\\ServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Watson\\Breadcrumbs\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
